{
  "name": "search-api",
  "version": "1.0.0",
  "description": "PaN search REST api",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t search-api .",
    "docker:run": "docker run -p 3000:3000 -d search-api",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node --inspect .\"",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^3.3.0",
    "@loopback/authentication-passport": "^1.1.0",
    "@loopback/boot": "^1.7.1",
    "@loopback/context": "^1.25.1",
    "@loopback/core": "^1.12.1",
    "@loopback/openapi-v3": "^1.11.0",
    "@loopback/repository": "^1.17.0",
    "@loopback/rest": "^1.25.1",
    "@loopback/rest-explorer": "^1.4.7",
    "@loopback/service-proxy": "^1.3.14",
    "@types/js-quantities": "^1.6.2",
    "@types/request-promise-native": "^1.0.17",
    "js-quantities": "^1.7.4",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-rest": "^3.6.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8"
  },
  "devDependencies": {
    "@loopback/build": "^2.1.0",
    "@loopback/eslint-config": "^4.1.5",
    "@loopback/testlab": "^1.10.1",
    "@types/node": "^10.17.13",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-eslint-plugin": "^2.2.0",
    "eslint-plugin-mocha": "^6.2.2",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.6.2",
    "tsc-watch": "^4.0.0",
    "typescript": "~3.6.4"
  }
}
